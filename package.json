{
  "name": "webpack-startup",
  "version": "1.0.0",
  "description": "webpack startup boilerplate",
  "repository": "https://github.com/bramdemmer/webpack-startup.git",
  "main": "index.js",
  "scripts": {
    "aem": "aemsync -t http://admin:admin@localhost:4502,http://admin:admin@localhost:4503 -w ./jcr_root/test",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config=webpack.dev.js ",
    "dev": "cross-env NODE_ENV=development webpack --config=webpack.dev.js",
    "prod": "cross-env NODE_ENV=production webpack --config=webpack.prod.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ],
    "*.scss": [
      "stylelint",
      "git add"
    ]
  },
  "keywords": [
    "webpack",
    "workflow",
    "starup"
  ],
  "author": "bram",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "aem-clientlib-webpack-plugin": "^1.6.0",
    "aemsync": "^3.0.2",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.3",
    "create-file-webpack": "^1.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "css-url-relative-plugin": "^1.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^4.6.0",
    "imagemin-webpack-plugin": "^2.4.0",
    "lint-staged": "^8.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-normalize-scss": "^8.0.0",
    "node-sass": "^4.11.0",
